<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
   <item android:id="@android:id/background" >
      <shape>
         <corners
         android:radius="5dp" />
         <gradient
         android:startColor="@color/ex_progress_default_bg"
         android:endColor="@color/ex_progress_default_bg" />
      </shape>
   </item>
   <item android:id="@android:id/progress" >
      <clip>
         <shape>
            <corners
            android:radius="5dp" />
            <gradient
            android:startColor="@color/ex_progress_bg"
            android:endColor="@color/ex_progress_bg" />
         </shape>
      </clip>
   </item>
</layer-list>
